{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-console": 0,
        "require-jsdoc": 0,
        "max-len": ["error", { "ignoreUrls": true, "ignoreTemplateLiterals": true, "ignoreComments": true, "ignoreStrings": true}],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "indent": ["error", 2, { "ignoredNodes": ["TemplateLiteral > *"] }],
        "new-cap": ["error", { "capIsNew": false }],
        "space-before-function-paren": 0
    }
}
